<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coearth">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- 인터넷 사용 권한 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Coearth">
        <activity android:name=".LifeActivity"></activity>
        <activity android:name=".CafeActivity" />
        <activity android:name=".FoodActivity"/>
        <activity android:name=".ClothActivity"/>
        <activity android:name=".EtcActivity"/>
        <activity android:name=".ReviewActivity" />
        <activity android:name=".DetailActivity" />

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="c0ywo7ygs0" />

        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashAcitvityTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>